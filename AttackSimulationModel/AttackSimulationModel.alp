					<Body><![CDATA[A_NetworkNode firstNode = findFirst(ap_NetworkNodes, n -> n.p_nodePosition == (int) Math.round(ap_NetworkNodes.getMinPos()));
firstNode.f_reset();
CompromisedChart.removeAll();
bc_compromisedCount.removeAll();

v_totalRuns = 0;
t_totalAttacksRun.setText(v_totalRuns);

v_totalROI = 0;

t_totalAttacksStopped.setText("0");
t_totalReturn.setText("0");
t_ROI.setText("0");
t_totalDataBreaches.setText("0");
t_totalLoss.setText("0");

t_bestControlName.setText("?");
t_bestControlROI.setText("?");
t_optimalControls.setText("?");

]]></Body>

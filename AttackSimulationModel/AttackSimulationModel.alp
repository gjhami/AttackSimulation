				<Function AccessType="default" StaticFunction="false">
					<ReturnModificator>VOID</ReturnModificator>
					<ReturnType><![CDATA[double]]></ReturnType>
					<Id>1653572803181</Id>
					<Name><![CDATA[f_updateGraph]]></Name>
					<X>540</X><Y>-430</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Body><![CDATA[NetworkNode node;
String nodeName;
double v_percentNodeCompromised;
double v_compromisedCount;
DataItem di_compromisedCount;
DataItem di_compromisedPercent;

// Clear Existing Chart Data
CompromisedChart.removeAll();
bc_compromisedCount.removeAll();


for (Map.Entry<Integer, NetworkNode> entry : v_positions.entrySet()) {
	node = entry.getValue();
	nodeName = node.getName();
	v_compromisedCount = (int) Math.round((double) node.v_numCompromises / (double) v_numYears);
	v_percentNodeCompromised = 100 * (v_compromisedCount / v_totalRuns);
   
    di_compromisedPercent = new DataItem();
    di_compromisedCount = new DataItem();
    
    // Used to assign different, random colors
	//int ColorIndex = entry.getKey() % 9;
	
	// Update percentage compromise chart
	di_compromisedPercent.setValue(v_percentNodeCompromised);
	CompromisedChart.addDataItem(di_compromisedPercent, nodeName, new Color(159, 168, 218));

	// Update count compromised chart
	di_compromisedCount.setValue(v_compromisedCount);
	bc_compromisedCount.addDataItem(di_compromisedCount, nodeName, new Color(159, 168, 218));
}

// Update totals for number of breaches, number of attacks stopped, ROI
// Compromises of the last node in the attack path are treated as data breaches
int numDataBreaches = (int) Math.round((double) (((NetworkNode) v_positions.lastEntry().getValue()).v_numCompromises) / (double) v_numYears);
double totalLoss = numDataBreaches * v_breachCost;
double numAttacksStopped = v_totalRuns - numDataBreaches;
double totalReturn = numAttacksStopped * v_breachCost;

t_totalDataBreaches.setText(numDataBreaches);
t_totalAttacksStopped.setText(v_totalRuns - numDataBreaches);

t_totalLoss.setText(totalLoss);
t_totalReturn.setText(totalReturn);
t_ROI.setText((totalReturn/v_totalCost) * 100);


]]></Body>

				<Function AccessType="default" StaticFunction="false">
					<ReturnModificator>RETURNS_VALUE</ReturnModificator>
					<ReturnType><![CDATA[double]]></ReturnType>
					<Id>1653922051174</Id>
					<Name><![CDATA[f_getEffectiveControlProbability]]></Name>
					<X>770</X><Y>-180</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Parameter>
						<Name><![CDATA[p_defaultProbability]]></Name>
						<Type><![CDATA[double]]></Type>
					</Parameter>
					<Parameter>
						<Name><![CDATA[p_defaultVariance]]></Name>
						<Type><![CDATA[double]]></Type>
					</Parameter>
					<Parameter>
						<Name><![CDATA[p_enabled]]></Name>
						<Type><![CDATA[boolean]]></Type>
					</Parameter>
					<Body><![CDATA[double v_effectiveProbability;

// If the control is not enabled, then its probability of stopping
// the attack is 0.
if (!p_enabled) {
	v_effectiveProbability = 0;
}
else {
	// Generate random variance between the positive and negative value of the variance
	double v_usedVariance;
	if (p_defaultVariance == 0) {
		v_usedVariance = 0;
	}
	else {
		double v_minVariance = -1 * p_defaultVariance;
		double v_maxVariance = 1 * p_defaultVariance;
		v_usedVariance = uniform(v_minVariance, v_maxVariance);
	}
	
	v_effectiveProbability = (p_defaultProbability + v_usedVariance);
	
	// Constrain bounds to 0 <= v_effectiveProbability <= 1
	if (v_effectiveProbability < 0) {
	    v_effectiveProbability = 0;
	}
	else if (v_effectiveProbability > 1) {
	    v_effectiveProbability = 1;
	}
}

return v_effectiveProbability;]]></Body>
				</Function>

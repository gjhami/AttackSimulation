				<Function AccessType="default" StaticFunction="false">
					<ReturnModificator>VOID</ReturnModificator>
					<ReturnType><![CDATA[double]]></ReturnType>
					<Id>1653572803181</Id>
					<Name><![CDATA[f_updateOutput]]></Name>
					<Description><![CDATA[Called at the end of a simulation from the f_runAttacks() function. Updates graph and text outputs of the simulation.]]></Description>
					<X>765</X><Y>-390</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Body><![CDATA[// Clear Existing Chart Data
CompromisedChart.removeAll();
bc_compromisedCount.removeAll();

// List of nodes with positions in the attack path
List<A_NetworkNode> nodes = filter(ap_NetworkNodes, n -> n.p_nodePosition > 0);

nodes.forEach((node) -> {
	// (int) Math.round() for a whole number
	v_compromisedCount = (double) node.v_numCompromises / (double) p_numYears;
	v_percentNodeCompromised = 100 * (((double) node.v_numCompromises / (double) p_numYears) / (double) v_totalRuns);
   
    di_compromisedPercent = new DataItem();
    di_compromisedCount = new DataItem();
    
    // Used to assign different, random colors
	//int ColorIndex = entry.getKey() % 9;
	
	// Update percentage compromise chart
	di_compromisedPercent.setValue(v_percentNodeCompromised);
	CompromisedChart.addDataItem(di_compromisedPercent, node.p_nodeName, new Color(159, 168, 218));

	// Update count compromised chart
	di_compromisedCount.setValue(v_compromisedCount);
	bc_compromisedCount.addDataItem(di_compromisedCount, node.p_nodeName, new Color(159, 168, 218));
});

// Update totals for number of breaches, number of attacks stopped, ROI
// Compromises of the last node in the attack path are treated as data breaches

A_NetworkNode lastNode = findFirst(ap_NetworkNodes, n -> n.p_nodePosition == (int) Math.round(ap_NetworkNodes.getMaxPos()));

double numDataBreaches = (double) (lastNode.v_numCompromises) / (double) p_numYears;
double totalLoss = numDataBreaches * p_breachCost;
double numAttacksStopped = v_totalRuns - ((double) (lastNode.v_numCompromises) / (double) p_numYears);

v_totalReturn = numAttacksStopped * p_breachCost;

t_totalDataBreaches.setText(numDataBreaches);
t_totalAttacksStopped.setText(v_totalRuns - numDataBreaches);

t_totalLoss.setText(totalLoss);
t_totalReturn.setText(v_totalReturn);
if (v_totalControlCost == 0) {
	v_totalROI = 0;
}
else {
	v_totalROI = (v_totalReturn/v_totalControlCost) * 100;
}
t_ROI.setText(v_totalROI);

f_updateTotalCost();
f_updateOutput();

//System.out.println("Return: " + v_totalReturn + ";\tCI: " + p_controlSetIndex + ";\tControls: " + cp_enabledControls);
]]></Body>

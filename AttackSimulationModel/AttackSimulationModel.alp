				<Function AccessType="default" StaticFunction="false">
					<ReturnModificator>VOID</ReturnModificator>
					<ReturnType><![CDATA[double]]></ReturnType>
					<Id>1481832035044</Id>
					<Name><![CDATA[f_calculateOverall]]></Name>
					<X>770</X><Y>-220</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Parameter>
						<Name><![CDATA[p_attackerFailRatio]]></Name>
						<Type><![CDATA[double]]></Type>
					</Parameter>
					<Body><![CDATA[double v_probabilityAttackSucceeds = 1;

v_effectiveFirewallProbability  = f_getEffectiveControlProbability(v_firewallPr, v_firewallVar, v_firewallEnabled);
v_probabilityAttackSucceeds     = v_probabilityAttackSucceeds * (1-v_effectiveFirewallProbability);

v_effectiveAntivirusProbability = f_getEffectiveControlProbability(v_antivirusPr, v_antivirusVar, v_antivirusEnabled);
v_probabilityAttackSucceeds     = v_probabilityAttackSucceeds * (1-v_effectiveAntivirusProbability);

v_effectiveIPSProbability       = f_getEffectiveControlProbability(v_IPSPr, v_IPSVar, v_IPSEnabled);
v_probabilityAttackSucceeds     = v_probabilityAttackSucceeds * (1-v_effectiveIPSProbability);

v_effectiveMailScanProbability  = f_getEffectiveControlProbability(v_mailScanPr, v_mailScanVar, v_mailScanEnabled);
v_probabilityAttackSucceeds     = v_probabilityAttackSucceeds * (1-v_effectiveMailScanProbability);

v_effectiveSecTrainingProbability  = f_getEffectiveControlProbability(v_secTrainingPr, v_secTrainingVar, v_secTrainingEnabled);
v_probabilityAttackSucceeds        = v_probabilityAttackSucceeds * (1-v_effectiveSecTrainingProbability);



// OverallPr is the probability of preventing an attack
// This is where the fail ratio of the attacker is incorporated
v_overallPr = (1 - v_probabilityAttackSucceeds) * p_attackerFailRatio;

if (v_overallPr > 1) {
    v_overallPr = 1;
}
else if (v_overallPr < 0) {
    v_overallPr = 0;
}

/*
This updates the probability of a successful attack to include the bounds
checking and incorporation of attacker fail ratio applied to OverallPr.
OverallPr is the probability the controls will stop the attack from compromising
this node.
*/
v_probabilityAttackSucceeds = 1 - v_overallPr;


//v_overallCost is the sum of the cost of all enabled controls, times the number of this node type in the network
v_overallCost = 0;
if (v_firewallEnabled == true) { v_overallCost = v_overallCost + v_firewallCost; }
if (v_IPSEnabled == true) { v_overallCost = v_overallCost + v_IPSCost; }
if (v_antivirusEnabled == true) { v_overallCost = v_overallCost + v_antivirusCost; }
if (v_mailScanEnabled == true) { v_overallCost = v_overallCost + v_mailScanCost; }
if (v_secTrainingEnabled == true) { v_overallCost = v_overallCost + v_secTrainingCost; }

v_overallCost = v_overallCost * v_nodeCount;

if(v_nextNode != null) {
	v_nextNode.f_calculateOverall(p_attackerFailRatio);
}]]></Body>
				</Function>

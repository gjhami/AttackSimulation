			<Functions>
				<Function AccessType="default" StaticFunction="false">
					<ReturnModificator>RETURNS_VALUE</ReturnModificator>
					<ReturnType><![CDATA[double]]></ReturnType>
					<Id>1655948896691</Id>
					<Name><![CDATA[f_getEffectivep]]></Name>
					<X>420</X><Y>-215</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Body><![CDATA[double v_mitigationp;

// If the control is not enabled, then its probability of stopping
// the attack is 0.
if (p_enabled == false) {
	v_mitigationp = 0;
}
else {
	// Generate random variance between the positive and negative value of the variance
	double v_mitigationv;
	if (p_mitigationV == 0) {
		v_mitigationv = 0;
	}
	else {
		double v_minv = -1 * p_mitigationV;
		double v_maxv = 1 * p_mitigationV;
		v_mitigationv = uniform(v_minv, v_maxv);
	}
	
	v_mitigationp = (p_mitigationP + v_mitigationv);
	
	// Constrain bounds to 0 <= v_effectiveProbability <= 1
	if (v_mitigationp < 0) {
	    v_mitigationp = 0;
	}
	else if (v_mitigationp > 1) {
	    v_mitigationp = 1;
	}
}

return v_mitigationp;]]></Body>
				</Function>

			<StartupCode><![CDATA[// Calculate default breach cost if not entered as
// a parameter
if (p_breachCost < 0) {
	f_updateBreachCost();
}

/* Set threat actors specified in parameter as selected */
for (ol_attackerIDs actorID : cp_selectedThreatActors) {
	findFirst(ap_ThreatActors, a -> a.p_id == actorID).f_setSelected(true);
}

/* Set controls specified in parameter as enabled */
for (Integer controlID : cp_enabledControls) {
	int nodeID = (int) selectFrom(control_inputs)
	.where(control_inputs.control_id.eq(controlID))
	.firstResult(control_inputs.node_id);
	
	A_NetworkNode node = findFirst(ap_NetworkNodes, n -> n.p_nodeID == nodeID);
	A_Control control = findFirst(node.ap_Controls, c -> c.p_controlID == controlID);
	control.cb_enabled.setSelected(true);
	control.p_enabled = true;
}

/* Run The Attack */
// f_runAttacks(p_attacksPerYear, p_numYears);]]></StartupCode>

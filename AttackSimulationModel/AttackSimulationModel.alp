					<Id>1491426884947</Id>
					<Name><![CDATA[f_linkNodes]]></Name>
					<X>540</X><Y>-90</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Body><![CDATA[Set set = v_positions.entrySet();
Iterator iterator = set.iterator();
NetworkNode PreviousNode = null;
NetworkNode CurrentNode = null;

while(iterator.hasNext()) {
	PreviousNode = CurrentNode;
	Map.Entry mentry = (Map.Entry)iterator.next();
	CurrentNode = (NetworkNode)mentry.getValue();
	
	if (PreviousNode != null) {
		PreviousNode.v_nextNode = CurrentNode;
	}
	if (!iterator.hasNext()) {
		CurrentNode.v_nextNode = null;
	}
}

]]></Body>

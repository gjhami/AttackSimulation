				<Function AccessType="default" StaticFunction="false">
					<ReturnModificator>VOID</ReturnModificator>
					<ReturnType><![CDATA[double]]></ReturnType>
					<Id>1491426884947</Id>
					<Name><![CDATA[f_linkNodes]]></Name>
					<Description><![CDATA[Sets the necessary references to build each node into a singly linked list representing the attack path. ]]></Description>
					<X>540</X><Y>-350</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Body><![CDATA[// Set current position to minimum position > 0
v_currentPosition = (int) Math.round(ap_NetworkNodes.getMinPos());
A_NetworkNode previousNode = findFirst(ap_NetworkNodes, n -> n.p_nodePosition == v_currentPosition);
A_NetworkNode currentNode;

// Increment current position until the max position is reached
while (v_currentPosition < (int) Math.round(ap_NetworkNodes.getMaxPos())) {
    v_currentPosition = v_currentPosition + 1;
    currentNode = findFirst(ap_NetworkNodes, n -> n.p_nodePosition == v_currentPosition);
    
    // If a node with the current position value is found
    if (currentNode != null) {
        previousNode.v_nextNode = currentNode;
        previousNode = currentNode;
    }
}

// Set the final node's nextNode to null
previousNode.v_nextNode = null;]]></Body>
				</Function>

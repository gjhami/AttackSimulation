				<Function AccessType="default" StaticFunction="false">
					<ReturnModificator>VOID</ReturnModificator>
					<ReturnType><![CDATA[double]]></ReturnType>
					<Id>1619668924016</Id>
					<Name><![CDATA[f_setNodePosition]]></Name>
					<X>540</X><Y>-110</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Parameter>
						<Name><![CDATA[box]]></Name>
						<Type><![CDATA[ShapeTextField]]></Type>
					</Parameter>
					<Parameter>
						<Name><![CDATA[node]]></Name>
						<Type><![CDATA[NetworkNode]]></Type>
					</Parameter>
					<Body><![CDATA[// Do not allow invalid integers or duplicate positions
Integer inputValue = null;
if(!box.getText().equals("")){
	
	try {
		inputValue = box.getIntValue();
	}
	catch (Exception e) {
		inputValue = null;
		box.setText("");
	}
	
	
	if ((inputValue != null) && (v_positions.containsKey(inputValue))) {
		box.setText("");
		inputValue = null;
	}
}

// If the  the node exists in Positions, then remove it
NetworkNode currNode;
Integer key;
Integer[] removeKeys = new Integer[v_positions.size()];
int index = 0;
if (v_positions.containsValue(node)) {
	for (Map.Entry<Integer, NetworkNode> entry : v_positions.entrySet()) {
		currNode = entry.getValue();
		key = entry.getKey();
		if (node.equals(currNode)) {
			removeKeys[index] = key;
		}
		else {
			removeKeys[index] = null;
		}
		index = index + 1;
	}
	for (index = 0; index < removeKeys.length; index++) {
		if (removeKeys[index] != null) {
			v_positions.remove(removeKeys[index]);
		}
	}
}

/*
If a position was supplied
	re-add the node with the new position
	visualize the node as part of the attack path
Else
	visualize the node as off
*/
if (inputValue != null) {
	v_positions.put(inputValue, node);
	node.v_inAttackPath = true;
}
else {
	node.v_inAttackPath = false;
}
PositionText.setText(v_positions.toString());





]]></Body>

				<Function AccessType="default" StaticFunction="false">
					<ReturnModificator>RETURNS_VALUE</ReturnModificator>
					<ReturnType><![CDATA[Set<Set<A_Control>>]]></ReturnType>
					<Id>1656085247849</Id>
					<Name><![CDATA[f_getPowerSet]]></Name>
					<Description><![CDATA[Given a set of controls, determines all possible combinations of those controls. These combinations are then tested in the f_optimizeReturn() and f_optimizeROI() functions so that the ideal subset of the set of possible controls for each metric can be determined.]]></Description>
					<X>990</X><Y>-570</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Parameter>
						<Name><![CDATA[p_originalSet]]></Name>
						<Type><![CDATA[Object]]></Type>
					</Parameter>
					<Body><![CDATA[/*
This code is a modified version of the function presented by Joao Silva
https://stackoverflow.com/questions/1670862/obtaining-a-powerset-of-a-set-in-java
*/
Set<A_Control> originalSet = (Set<A_Control>) p_originalSet;

Set<Set<A_Control>> sets = new HashSet<Set<A_Control>>();
if (originalSet.isEmpty()) {
    sets.add(new HashSet<A_Control>());
    return sets;
}
List<A_Control> list = new ArrayList<A_Control>(originalSet);
A_Control head = list.get(0);
Set<A_Control> rest = new HashSet<A_Control>(list.subList(1, list.size())); 
for (Set<A_Control> set : f_getPowerSet(rest)) {
    Set<A_Control> newSet = new HashSet<A_Control>();
    newSet.add(head);
    newSet.addAll(set);
    sets.add(newSet);
    sets.add(set);
}       
return sets;]]></Body>
				</Function>
				<Function AccessType="default" StaticFunction="false">
					<ReturnModificator>VOID</ReturnModificator>
					<ReturnType><![CDATA[double]]></ReturnType>
					<Id>1656085369880</Id>
					<Name><![CDATA[f_optimizeReturn]]></Name>
					<Description><![CDATA[Called when the user clicks the optimize button. Determines the set of controls with the highest total return that is within the user's budget and updates outputs to reflect the optimized set of controls.]]></Description>
					<X>990</X><Y>-590</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Body><![CDATA[cv_allControls = f_getAllControls();
cv_controlsPowerSet = new ArrayList<Set>(f_getPowerSet(cv_allControls));
Set<A_Control> v_bestControlSet = new HashSet<A_Control>();
v_maxReturn = -1;

// Get nodes in the attack path
List<A_NetworkNode> nodes = filter(ap_NetworkNodes, n -> n.p_nodePosition > 0);

// For Each possible set of controls
for (Set<A_Control> controlSet : cv_controlsPowerSet) {

	// Disable any controls already enabled
	nodes.forEach((node) -> {
		List<A_Control> controls = filter(node.ap_Controls, c -> c.p_enabled == true);
		controls.forEach((control) -> {
			control.cb_enabled.setSelected(false);
			control.p_enabled = false;
		});
	});
	

	// Reset aggregate variables to test control set from clean state
	f_reset();

	// Enable controls in the set
	for (A_Control control : controlSet) {
		control.cb_enabled.setSelected(true);
		control.p_enabled = true;
	}
	
	// Run the attack
	f_runAttacks(p_attacksPerYear, p_numYears);
	
	// If the return is higher than the max return and the control cost is under budget
	if ((v_totalReturn > v_maxReturn) && (ap_NetworkNodes.getOverallCost() <= p_annualBudget)) {
		// Then save this as the best control set yet
		v_maxReturn = v_totalReturn;
		v_bestControlSet = controlSet;
	}
	
	
	// Print Debug
	// System.out.println("\t\tReturn: " + v_totalReturn + ";\t\t" + controlSet);
}

// Re-run the attack with the best control set to visualize
// Data for this control set
f_reset();

// Disable any controls already enabled
nodes.forEach((node) -> {
	List<A_Control> controls = filter(node.ap_Controls, c -> c.p_enabled == true);
	controls.forEach((control) -> {
		control.cb_enabled.setSelected(false);
		control.p_enabled = false;
	});
});

// Enable best set of controls
v_optimalControlsString = "";
for (A_Control control : v_bestControlSet) {
	control.cb_enabled.setSelected(true);
	control.p_enabled = true;
	nodes.forEach((node) -> {
		List<A_Control> controls = filter(node.ap_Controls, c -> c.p_controlID == control.p_controlID);
		
		if (controls.size() > 0) {
			v_optimalControlsString = v_optimalControlsString + node.p_nodeName + ":";
		
			controls.forEach((c) -> {
				v_optimalControlsString = v_optimalControlsString + " " + c.p_controlName + ",";
			});
			
			v_optimalControlsString = v_optimalControlsString + "\n";
		}
	});
	
	t_optimalControls.setText(v_optimalControlsString);
}

f_runAttacks(p_attacksPerYear, p_numYears);
t_bestControlName.setText(v_optimalROIString);
t_bestControlROI.setText("%" + v_maxROI);
]]></Body>

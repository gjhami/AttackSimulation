			<Functions>
				<Function AccessType="default" StaticFunction="false">
					<ReturnModificator>VOID</ReturnModificator>
					<ReturnType><![CDATA[double]]></ReturnType>
					<Id>1481832035044</Id>
					<Name><![CDATA[f_calculateOverall]]></Name>
					<Description><![CDATA[Calculates the overall probability of an attack being stopped at the current node and perform the same calculation at the next node in the attack path. This value is then used in the action chart to determine if the node was compromised. ]]></Description>
					<X>470</X><Y>-170</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Parameter>
						<Name><![CDATA[p_attackerFailRatio]]></Name>
						<Type><![CDATA[double]]></Type>
					</Parameter>
					<Body><![CDATA[v_probabilityAttackSucceeds = 1;
v_overallCost = 0;

List<A_Control> controls = filter(ap_Controls, c -> c.p_enabled == true);

controls.forEach((control) -> {
	v_probabilityAttackSucceeds = v_probabilityAttackSucceeds * (1 - control.f_getEffectivep());
	v_overallCost = v_overallCost + control.p_annualCost;
});

// OverallPr is the probability of preventing an attack
// This is where the fail ratio of the attacker is incorporated
v_overallPr = (1 - v_probabilityAttackSucceeds) * p_attackerFailRatio;

if (v_overallPr > 1) {
    v_overallPr = 1;
}
else if (v_overallPr < 0) {
    v_overallPr = 0;
}

/*
This updates the probability of a successful attack to include the bounds
checking and incorporation of attacker fail ratio applied to OverallPr.
OverallPr is the probability the controls will stop the attack from compromising
this node.
*/
v_probabilityAttackSucceeds = 1 - v_overallPr;

v_overallCost = v_overallCost * p_nodeCount;

if(v_nextNode != null) {
	v_nextNode.f_calculateOverall(p_attackerFailRatio);
}]]></Body>
				</Function>
